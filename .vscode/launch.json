    "version": "0.2.0",
    "configurations": [
        {
            "name": "diet_tracker_app",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "diet_tracker_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "diet_tracker_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "diet_tracker_app (web mode - canvaskit)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--web-renderer",
                "canvaskit",
            ]
        },
        {
            "name": "diet_tracker_app (web mode - web renderer)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "-d",
                "chrome",
                "--web-renderer",
                "html",
            ]
        }
    ]
}